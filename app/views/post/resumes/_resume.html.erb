  <tr>
    <td> <%= resume.content %> </td>
    <!-- <td> <%= link_to("Download", resume.attachment_url) %></td> -->
    <td>
        <%= link_to post_resumes_path do %>
          <% if resume.image.present? %>
            <%= image_tag(resume.image.thumb.url, class: "thumbnail") %>
          <% else %>
            <%= image_tag("http://placehold.it/200x200&text=No Pic", class: "thumbnail") %>
          <% end %>
        <% end %>
    </td>
    <td> <%= resume.job.title %>
    <td> <%= resume.created_at %> </td>
    <td> <%= link_to('Edit', edit_job_resume_path(resume.job, resume), class: "btn btn-default btn-xs") %></td>
    <td> <%= link_to('Delete', job_resume_path(resume.job, resume), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-default btn-xs") %></td>
  </tr>
